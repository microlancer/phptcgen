{
    "name": "thorie7912/phptcgen",
    "type": "tool",
    "description": "A simple tool for generating PHPUnit test cases based on PHP DocBlock comments",
    "keywords": ["docblock", "reflection", "unit test", "phpunit", "test case", "generator"],
    "homepage": "https://github.com/thorie7912/phptcgen",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Horie",
            "email": "tak@thorie.com",
            "homepage": "http://thorie.com/blog",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "dannykopping/docblock": ">=0.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": { "DocBlock\\": "DocBlock" }
    }
}